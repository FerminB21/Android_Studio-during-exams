<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".chest_easy"
    >

    <ImageView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:scaleType="fitXY"
        android:src="@drawable/back"
        />
    <TextView
        android:id="@+id/abs_exercise_rounds"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="repeat warm up set 2-3 times"
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/abs_exercise1_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="• 5 reps of push ups"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:layout_marginTop="40dp"
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/abs_exercise2_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="• jumping jacks for 1 minute"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:layout_marginTop="60dp"
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/abs_exercise4_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="repeat first set 4-6 times "
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:layout_marginTop="90dp"
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/abs_exercise5_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="• 10 reps of push ups "
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:layout_marginTop="120dp"
        android:layout_marginLeft="10dp"
        />

    <TextView
        android:id="@+id/abs_exercise6_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="• 10 reps of narrow push ups "
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:layout_marginTop="140dp"
        android:layout_marginLeft="10dp"
        />


    <TextView
        android:id="@+id/abs_exercise8_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="repeat second set 4-6 times "
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:layout_marginTop="180dp"
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/abs_exercise7_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="•  10 pike push ups  "
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:layout_marginTop="210dp"
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/arm_exercise8_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="• 5 reps of diamond push ups"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:layout_marginTop="230dp"
        android:layout_marginLeft="10dp"
        />


    <TextView
        android:id="@+id/abs_exercise10_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="repeat third set  4 times "
        android:textColor="#FFFFFF"
        android:textSize="20sp"
        android:layout_marginTop="270dp"
        android:layout_marginLeft="10dp"
        />
    <TextView
        android:id="@+id/arm_exercise11_hard"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="• 7 reps of slow push ups"
        android:textColor="#FFFFFF"
        android:textSize="15sp"
        android:layout_marginTop="300dp"
        android:layout_marginLeft="10dp"
        />
    <Button
        android:id="@+id/end"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="130dp"
        android:layout_marginTop="380dp"
        android:text="end of exercises" />


</RelativeLayout>